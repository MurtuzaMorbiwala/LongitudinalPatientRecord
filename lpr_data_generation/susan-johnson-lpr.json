  "ClinicalNotes": [
    {
      "id": "note-001",
      "patient_id": "patient-003",
      "encounter_id": "encounter-001",
      "related_condition_id": "condition-002",
      "related_procedure_id": null,
      "related_observation_id": "observation-003",
      "related_medication_id": "med-request-003",
      "author": "Dr. Lisa Martinez",
      "note_text": "48-year-old female presents for anxiety follow-up. Reports moderate improvement with sertraline 50mg daily. GAD-7 score is 8, down from 12 at last visit. Still experiencing occasional panic symptoms, particularly during stressful work periods. Sleep has improved. Continuing current medications. Recommended continuing with CBT and stress reduction techniques. Follow-up in 3 months.",
      "created_on": "2023-02-20T10:45:00",
      "note_type": "progress note"
    },
    {
      "id": "note-002",
      "patient_id": "patient-003",
      "encounter_id": "encounter-002",
      "related_procedure_id": "procedure-001",
      "related_condition_id": "condition-001",
      "related_observation_id": null,
      "related_medication_id": null,
      "author": "Dr. Robert Wilson",
      "note_text": "Patient underwent CT scan of sinuses for evaluation of chronic headaches and suspected sinusitis. Findings: No evidence of significant sinus disease. Mild mucosal thickening in the left maxillary sinus, likely representing mild inflammatory changes. No air-fluid levels, masses, or bony destruction identified. Impression: Minimal sinus disease, unlikely to be primary source of patient's headaches.",
      "created_on": "2019-08-05T10:15:00",
      "note_type": "procedure note"
    },
    {
      "id": "note-003",
      "patient_id": "patient-003",
      "encounter_id": "encounter-004",
      "related_condition_id": "condition-001",
      "related_procedure_id": null,
      "related_observation_id": null,
      "related_medication_id": "med-request-001",
      "author": "Dr. David Miller",
      "note_text": "Neurology consultation for chronic migraine management. Patient reports 3-4 migraines per month, typically lasting 8-12 hours. Identified triggers include stress, lack of sleep, and certain foods. Current abortive therapy with sumatriptan 50mg provides relief if taken early. Discussed prophylactic options. Plan to continue current regimen and add migraine diary. Return in 3 months for follow-up.",
      "created_on": "2019-11-15T13:50:00",
      "note_type": "consultation note"
    },
    {
      "id": "note-004",
      "patient_id": "patient-003",
      "encounter_id": "encounter-005",
      "related_condition_id": "condition-003",
      "related_procedure_id": null,
      "related_observation_id": "observation-001",
      "related_medication_id": "med-request-002",
      "author": "Dr. Jennifer Garcia",
      "note_text": "Annual physical examination. Thyroid function is well-controlled on current levothyroxine dose with TSH at 2.8 mIU/L. Patient reports good energy levels. Blood pressure 118/76, BMI 24.2. All other labs within normal limits. Continuing current medications. Recommended age-appropriate cancer screenings and influenza vaccination.",
